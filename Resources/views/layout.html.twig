{% extends "::base.html.twig" %}

{% trans_default_domain 'asf_document' %}

{% block stylesheets %}
{{ parent() }}
{% stylesheets '@ASFDocumentBundle/Resources/public/less/*' %}
	<link href="{{ asset_url }}" rel="stylesheet" type="text/css" />
{% endstylesheets %}
{% endblock %}

{% block javascripts %}
{{ parent() }}
{% javascripts '@ASFDocumentBundle/Resources/public/js/*' %}
	<script src="{{ asset_url }}"></script>
{% endjavascripts %}
{% endblock %}

{% block body %}
	{% block asf_doc_content %}{% endblock %}
{% endblock %}