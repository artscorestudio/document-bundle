{
	"name" : "artscorestudio/document-bundle",
	"type" : "library",
	"description" : "Document Bundle is a Symfony 2/3 bundle for create and manage documents in your Symfony 2/3 application. This package is a part of Artscore Studio Framework.",
	"keywords" : [
		"Symfony"
	],
	"homepage" : "https://github.com/artscorestudio/document-bundle",
	"license" : "MIT",
	"authors" : [{
			"name" : "Nicolas Claverie",
			"email" : "info@artscore-studio.fr"
		}, {
			"name" : "Artscore Studio",
			"homepage" : "http://www.artscore-studio.fr"
		}
	],
	"require" : {
		"php": ">=5.5.9",
		
        "symfony/framework-bundle": "~2.8|~3.0",
        "symfony/expression-language": "~2.8|~3.0",
        "symfony/browser-kit": "~2.8|~3.0",
        "symfony/css-selector": "~2.8|~3.0",
        "symfony/twig-bundle": "~2.3|~3.0",
        "symfony/event-dispatcher": "~2.8|~3.0",
        "symfony/form": "~2.3|~3.0",
        "symfony/assetic-bundle": "^2.7",
        
        "doctrine/orm": "~2.4|^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "~1.0|^1.2",
        
        "twig/twig": "~1.10",
        
        "artscorestudio/core-bundle": "dev-master"
	},
	"require-dev" : {
	    "friendsofsymfony/user-bundle": "~2.0@dev",
	    "symfony/security": "~2.8|~3.0",
        "symfony/security-csrf": "~2.8|~3.0",
        "symfony/swiftmailer-bundle": "^2.3",
        "artscorestudio/user-bundle": "dev-master"
	},
	"autoload" : {
		"psr-4" : {
			"ASF\\DocumentBundle\\" : ""
		}
	},
	"minimum-stability" : "dev"
}